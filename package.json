{
  "name": "snake-typescript",
  "version": "1.0.0",
  "description": "snake game",
  "main": "index.js",
  "scripts": {
    "compile": "tsc -p tsconfig.json",
    "start:dev": "parcel index.html",
    "test": "jest"
  },
  "keywords": [
    "typescript",
    "react",
    "snake",
    "game"
  ],
  "author": "Luis Valdovinos",
  "license": "ISC",
  "devDependencies": {
    "@types/css-modules": "^1.0.0",
    "@types/enzyme": "^3.1.10",
    "@types/enzyme-adapter-react-16": "^1.0.2",
    "@types/jest": "^22.2.3",
    "@types/node": "^9.6.5",
    "@types/react": "^16.3.11",
    "@types/react-dom": "^16.0.5",
    "autoprefixer": "^8.3.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.4.3",
    "parcel-bundler": "^1.7.1",
    "postcss-modules": "^1.1.0",
    "ts-jest": "^22.4.3",
    "tslint": "^5.9.1",
    "typescript": "^2.8.1"
  },
  "dependencies": {
    "react": "^16.3.2",
    "react-dom": "^16.3.2"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ],
    "setupFiles": [
      "./beforeTest.ts"
    ]
  }
}
